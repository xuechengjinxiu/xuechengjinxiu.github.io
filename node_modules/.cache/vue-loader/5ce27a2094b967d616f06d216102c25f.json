ss=\"mt-12 mb-8\" src=\"@/assets/ico_success.svg\" />\n      <p class=\"SF-Display-Regular\">You Sent {{ amount }} to</p>\n      <p class=\"text-break px-11 font20\" style=\"margin: 6px 0 48px 0\">\n        {{ recipient }}\n      </p>\n      <p class=\"font14 pb-8 color-grey\">\n        Will auto turn to home page in {{ timeNumber }}s\n      </p>\n    </v-card>\n    <router-link to=\"/home\">\n      <v-btn\n        block\n        height=\"52px\"\n        class=\"text-none primary white--text mt-6 mb-4 SF-Text-Medium\"\n        style=\"border-radius: 8px\"\n      >\n        <div class=\"font18\">Back to Home</div>\n      </v-btn>\n    </router-link>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      interval: \"\",\n      amount: 0,\n      recipient: \"\",\n      timeNumber: 5\n    };\n  },\n  created() {\n    console.log(\"countdown start\");\n    this.recipient = this.$store.state.sendInfo.recipient;\n    this.amount = this.$store.state.sendInfo.amount;\n    this.setEndTime();\n  },\n  methods: {\n    setEndTime() {\n      let that = this;\n      this.interval = setInterval(function countTime() {\n        // console.log(that.timeNumber);\n        that.timeNumber--;\n        if (that.timeNumber <= 0) {\n          clearInterval(this.interval);\n          that.$store.commit(\"reset_sendinfo\");\n          that.$router.replace({ path: \"/home\" });\n        }\n      }, 1000);\n    }\n  },\n  beforeDestroy() {\n    clearInterval(this.interval);\n  }\n};\n</script>\n\n<style scoped>\n.v-card {\n  box-shadow: none;\n  -webkit-box-shadow: none;\n}\n</style>\n"]}]}