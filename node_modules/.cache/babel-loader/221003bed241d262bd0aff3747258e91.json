   <v-btn\n                class=\"text-none\"\n                color=\"grey\"\n                text\n                @click=\"dialog = false\"\n              >\n                Cancel\n              </v-btn>\n            </div>\n            <div class=\"flex-grow-1 setting-btn2 set-flex\">\n              <v-btn\n                class=\"text-none\"\n                color=\"warning\"\n                text\n                @click=\"\n                  dialog = false;\n                  resetWallet();\n                \"\n              >\n                Reset\n              </v-btn>\n            </div>\n          </v-card-actions>\n        </v-card>\n      </v-overlay>\n    </div>\n  </div>\n  <!--  </div>-->\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      langs: [\n        {\n          value: \"English\"\n        },\n        {\n          value: \"繁体中文\"\n        },\n        {\n          value: \"한국어\"\n        }\n      ],\n      times: [5, 10, 15],\n      //default lang English\n      selectedLang: \"English\",\n      selectedTime: Number(window.localStorage[\"sessionTimeout\"]),\n      isDisabled: false,\n      dialog: false,\n      dialog1: false,\n      dialog2: false\n    };\n  },\n  methods: {\n    resetWallet() {\n      this.$store.dispatch(\"reset_state\").then(() => {\n        window.localStorage.clear();\n        this.$router.replace({ path: \"/create\" });\n      });\n    },\n    langDisabled(lang) {\n      return lang === this.langs[1].value || lang === this.langs[2].value;\n    },\n    selectLang(lang) {\n      if (this.langDisabled(lang)) return;\n      this.selectedLang = lang;\n      this.dialog1 = false;\n    },\n    selectTime(time) {\n      this.selectedTime = time;\n      window.localStorage[\"sessionTimeout\"] = time;\n      this.dialog2 = false;\n    }\n  }\n};\n</script>\n<style scoped>\n.v-setting {\n  border-radius: 8px;\n}\n.setting-btn1,\n.setting-btn2 {\n  border-top: 1px solid #f2f2f2;\n  height: 56px;\n}\n.sett