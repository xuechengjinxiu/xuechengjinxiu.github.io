color: var(--v-error-base);font-size: 12px; height: 24px\">\n        <p class=\"px-3\" v-show=\"checkPromp\">{{ checkPromp }}</p>\n      </div>\n      <v-btn\n        block\n        color=\"primary\"\n        height=\"48px\"\n        class=\"text-none white btn\"\n        @click=\"confirm()\"\n      >\n        Confirm\n      </v-btn>\n      <router-link to=\"createsecond\">\n        <v-btn\n          block\n          height=\"48px\"\n          class=\"text-none white primary--text mt-4 btn btn-border SF-Text-Medium\"\n        >\n          See Seed\n        </v-btn>\n      </router-link>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      seedWord: [],\n      seedIndex: {},\n      showSeed: [],\n      showSeed1: [],\n      checkPromp: \"\"\n    };\n  },\n  methods: {\n    wordbtn(index, item) {\n      if (!this.showSeed.includes(index)) {\n        this.showSeed.push(index);\n        this.showSeed1.push(item);\n      } else {\n        this.showSeed.splice(\n          this.showSeed.findIndex(i => i === index),\n          1\n        );\n        this.showSeed1.splice(\n          this.showSeed1.findIndex(i => i === item),\n          1\n        );\n      }\n      this.seedIndex[index] = !this.seedIndex[index];\n    },\n    confirm() {\n      if (this.seedWord == this.showSeed1.toString()) {\n        if (this.$store.state.loginType === \"send\") {\n          this.$router.replace({\n            path: \"/home/shift\",\n            query: this.$route.query\n          });\n        } else {\n          this.$router.replace({ path: \"/home\" });\n        }\n      } else {\n        // alert(\"You should click word in order\");\n        this.checkPromp = \"You should click word in order.\";\n      }\n    }\n  },\n  created() {\n    this.seedWord = this.$store.state.seed.split(\" \");\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n.item-word {\n  padding: 1vh;\n  height: fit-content;\n  border-radius: 1vh;\n}\n.btn {\n  border-radius: 8px;\n  font-size: 18px;\n}\n.btn2 {\n  color: #9e9e9e;\n}\n</style>\n"],"sourceRoot":"src/views/Create"}]}