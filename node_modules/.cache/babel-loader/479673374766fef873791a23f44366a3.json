OAAA,CAAA,OAAA;AACA,WAAA,cAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA,CALA,CAMA;;AACA,WAAA,UAAA,IAAA,CAAA;AACA,KA3FA;AA4FA,IAAA,cA5FA,4BA4FA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,YADA;AAEA,QAAA,OAAA,EAAA,KAAA,OAAA,CAAA;AAFA,OAAA;AAIA,KAjGA;AAkGA,IAAA,iBAlGA,6BAkGA,IAlGA,EAkGA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,WAAA;AADA,OAAA;AAGA,KAtGA;AAuGA;AACA,IAAA,UAxGA,wBAwGA;AACA,WAAA,cAAA,GAAA,CAAA,KAAA,cAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,YAAA,CAAA,OAAA,CAAA,CAAA;AACA,UAAA,KAAA,GAAA,KAAA,YAAA;;AACA,UAAA,KAAA,KAAA,CAAA,MAAA,IAAA,EAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,GAAA,KAAA,KAAA;AACA,QAAA,MAAA,CAAA,YAAA,CAAA,OAAA,IAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,eAAA,EAAA;AACA,UAAA,KAAA,EAAA,KADA;AAEA,UAAA,KAAA,EAAA,KAAA,CAAA,KAAA;AAFA,SAAA;AAIA,OAPA,MAOA;AACA;AACA,aAAA,KAAA,GAAA,KAAA,CAAA,KAAA,CAAA;AACA;AACA,KAvHA;AAwHA;AACA,IAAA,UAzHA,wBAyHA;AACA,WAAA,WAAA,GAAA,CAAA,KAAA,WAAA;AACA,WAAA,YAAA,GAAA,CAAA,KAAA,YAAA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,KA7HA;AA8HA,IAAA,cA9HA;AAAA;AAAA;AAAA,gDA8HA,OA9HA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgIA,qBAAA,YAAA,GAAA,IAAA;AAhIA;AAAA,uBAiIA,OAjIA;;AAAA;AAkIA,qBAAA,YAAA,GAAA,IAAA;AAlIA;AAAA;;AAAA;AAAA;AAAA;AAoIA,qBAAA,WAAA,GAAA,KAAA;;AACA,oBAAA,aAAA,IAAA,KAAA,iBAAA,EAAA;AACA,kBAAA,KAAA,CAAA,uCAAA,CAAA;AACA,iBAFA,MAEA,IAAA,aAAA,IAAA,KAAA,eAAA,EAAA;AACA,kBAAA,KAAA,CAAA,qCAAA,CAAA;AACA,iBAFA,MAEA,IAAA,aAAA,IAAA,KAAA,mBAAA,EAAA;AACA,kBAAA,KAAA,CAAA,8CAAA,CAAA;AACA,iBAFA,MAEA,IAAA,aAAA,IAAA,KAAA,kBAAA,EAAA;AACA,kBAAA,KAAA,CAAA,gCAAA,CAAA;AACA,iBAFA,MAEA,IAAA,aAAA,IAAA,KAAA,sBAAA,EAAA;AACA,kBAAA,KAAA,CAAA,0CAAA,CAAA;AACA,iBAFA,MAEA;AACA,kBAAA,KAAA,cAAA;AACA;;AAjJA;AAAA;AAmJA,qBAAA,YAAA,GAAA,KAAA;AAnJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAsJA,IAAA,UAtJA,sBAsJA,YAtJA,EAsJA;AACA,UAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,YAAA,CAAA,YAAA,EAAA;AACA,YAAA,cAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,cAAA;;AACA,YACA,MAAA,CAAA,cAAA,CAAA,cAAA,CAAA,OAAA,EAAA,KAAA,OAAA,CAAA,QAAA,CADA,EAEA;AACA,eAAA,MAAA,CAAA,MAAA,CAAA,iBAAA,EAAA;AACA,YAAA,SAAA,EAAA,cAAA,CAAA,OADA;AAEA,YAAA,MAAA,EAAA,cAAA,CAAA,MAFA;AAGA,YAAA,WAAA,EAAA,cAAA,CAAA;AAHA,WAAA;AAKA,eAAA,OAAA,CAAA,OAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AACA;AACA,OAfA,CAeA,OAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,GAAA,EADA,CAEA;;AACA,aAAA,YAAA,GAAA,MAAA;AACA;AACA;AA3KA,GA/CA;AA6NA,EAAA,UAAA,EAAA;AACA,IAAA,eAAA,EAAA,eADA;AAEA,IAAA,OAAA,EAAA,OAFA;AAGA,IAAA,YAAA,EAAA,YAHA;AAIA,IAAA,WAAA,EAAA;AAJA;AA7NA,CAAA","sourcesContent":["<template>\n  <!-- show Wallet -->\n  <div class=\"maxWidth\">\n    <div class=\"mt-6 mb-6\">\n      <!-- side drawer for mobile -->\n      <div\n        class=\"wallet-bg white--text d-sm-none\"\n        style=\"padding: 8.533vw 5.33vw 5.33vw 5.33vw\"\n      >\n        <p\n          class=\"SF-Display-Medium d-sm-none\"\n          style=\"font-size: 4vw; line-height:4.8vw;\"\n        >\n          Total Balance\n        </p>\n        <div class=\"bal-outer\" style=\"margin: 2.13vw 0 1.6vw 0\">\n          <p class=\"title-balance d-sm-none bal-inner DIN-Alternate-Bold\">\n            {{ $utils.toThousands(balance) }}\n          </p>\n        </div>\n        <div class=\"SF-Display-Light d-sm-none\" style=\"font-size: 3.4vw\">\n          <span id=\"copy-address\" class=\"text-break\">{{ address }}</span\n          >&nbsp;\n          <copy-button copy-id=\"copy-address\"></copy-button>\n          <div>\n            Alias :&nbsp;\n            <input\n              v-show=\"editingAddress\"\n              class=\"edi-address\"\n              maxlength=\"15\"\n              v-model=\"alias\"\n              @blur=\"storeAlias()\"\n            />\n            <span v-show=\"!editingAddress\"> {{ alias }}&nbsp; </span>\n            <v-icon\n              class=\"pb-1\"\n              small\n              color=\"white\"\n              @click=\"editingAddress = !editingAddress\"\n            >\n              mdi-square-edit-outline\n            </v-icon>\n          </div>\n        </div>\n      </div>\n      <!-- desktop mode for pc -->\n      <div class=\"col-12 pa-6 wallet-bg white--text d-none d-sm-block\">\n        <p class=\"SF-Display-Medium d-none d-sm-flex display-2\">\n          Total Balance\n        </p>\n        <p class=\"d-none d-sm-flex display-4 font-weight-medium\">\n          <span class=\"DIN-Alternate-Bold\">{{\n            $utils.toThousands(balance)\n          }}</span>\n        </p>\n        <div class=\"SF-Display-Light d-none d-sm-block display-1\">\n          <span id=\"copy-address1\" class=\"text-break\">{{ address }}</span\n          >&nbsp;\n          <copy-button copy-id=\"copy-address1\"></copy-button>\n          <div class=\"mt-2\">\n            Alias :&nbsp;\n            <input\n              v-show=\"editingAddress\"\n              style=\"outline: none;border-bottom: 1px solid white;\"\n              maxlength=\"15\"\n              v-model=\"alias\"\n              @blur=\"storeAlias()\"\n            />\n            <span v-show=\"!editingAddress\"> {{ alias }}&nbsp; </span>\n            <v-icon\n              class=\"pb-1\"\n              middle\n              color=\"white\"\n              @click=\"editingAddress = !editingAddress\"\n            >\n              mdi-square-edit-outline\n            </v-icon>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- double button for transaction -->\n    <div class=\"mx4 mt-6 mb-6 d-flex btn-tran SF-Text-Medium\">\n      <div class=\"mr-1 py-3 text-center\" @click=\"dialog = !dialog\">\n        <img\n          :src=\"require(`@/assets/${$config.theme}/ic_send.svg`)\"\n          style=\"vertical-align: text-top;\"\n        />\n        &nbsp;&nbsp;\n        <span>Send</span>\n      </div>\n      <div class=\"ml-1 py-3 text-center\" @click=\"routerTransaction('receive')\">\n        <img\n          :src=\"require(`@/assets/${$config.theme}/ic_receive.svg`)\"\n          style=\"vertical-align: text-bottom;\"\n        />\n        &nbsp;&nbsp;\n        <span>Receive</span>\n      </div>\n    </div>\n\n    <!-- transaction dialog -->\n    <v-dialog v-model=\"dialog\" transition=\"dialog-bottom-transition\">\n      <v-card class=\"text-center SF-Text-Regular\" style=\"border-radius: 12px\">\n        <p\n          class=\"send-card\"\n          @click=\"\n            dialog = !dialog;\n            scanOverlay = !scanOverlay;\n            openCamera = 'auto';\n          \"\n        >\n          Send with Qr Scaning\n        </p>\n        <p class=\"mb-0 send-card\" @click=\"routerTransaction('send')\">\n          Send Manualy\n        </p>\n      </v-card>\n    </v-dialog>\n\n    <!-- scan function -->\n    <scanQrStyle v-show=\"scanOverlay1\" />\n    <div v-if=\"scanOverlay\" class=\"scan-style\">\n      <qrcode-stream\n        @init=\"onQrImportInit\"\n        @decode=\"onQrDecode\"\n        :camera=\"openCamera\"\n      >\n        <img\n          v-if=\"qrImportInit\"\n          class=\"qrcode-waiting\"\n          height=\"100\"\n          width=\"100\"\n          src=\"@/assets/ic_wait.svg\"\n        />\n      </qrcode-stream>\n    </div>\n\n    <!-- Transaction record menu -->\n    <div class=\"mt-6 mb-6\">\n      <div class=\"white add-radius px-5 py-6\" ref=\"records-list\">\n        <div class=\"d-flex justify-space-between align-center mb-2\">\n          <span class=\"SF-Display-Semibold font22\">Records</span>\n          <div class=\"d-flex align-center\">\n            <div>\n              <v-menu offset-y min-width=\"100\" v-model=\"menuOn\">\n                <template v-slot:activator=\"{ on }\">\n                  <div\n                    v-on=\"on\"\n                    class=\"d-flex add-radius\"\n                    :class=\"{\n                      'records-off': !menuOn,\n                      'white-font': menuOn,\n                      primary: menuOn\n                    }\"\n                  >\n                    <span class=\"record-menu-l SF-Text-Regular\">{{\n                      typetransation\n                    }}</span>\n                    <v-icon\n                      class=\"record-menu-r\"\n                      :class=\"{\n                        'grey-font': !menuOn,\n                        'white-font': menuOn\n                      }\"\n                      >mdi-menu-down</v-icon\n                    >\n                  </div>\n                </template>\n                <v-list dense>\n                  <v-list-item\n                    v-for=\"(item, index) in txTypes\"\n                    :key=\"index\"\n                    @click=\"typeChange(item)\"\n                    :disabled=\"isloading\"\n                  >\n                    <v-list-item-title>\n                      <span\n                        class=\"SF-Text-Regular font14\"\n                        :class=\"{ 'primary--text': item == typetransation }\"\n                      >\n                        {{ item }}\n                      </span>\n                    </v-list-item-title>\n                  </v-list-item>\n                </v-list>\n              </v-menu>\n            </div>\n            <img\n              class=\"ml-4 refresh\"\n              src=\"@/assets/ic_refresh.svg\"\n              @click=\"refreshData()\"\n            />\n          </div>\n        </div>\n\n        <!-- list -->\n        <!-- <div v-show=\"loading\" class=\"text-center mt-8\">\n          <v-progress-circular\n            :size=\"70\"\n            :width=\"5\"\n            color=\"primary\"\n            indeterminate\n          >\n          </v-progress-circular>\n        </div> -->\n        <div class=\"mt-4 SF-Text-Regular\" style=\"overflow-x: hidden;\">\n          <v-list-group\n            :ripple=\"false\"\n            no-action\n            color=\"none\"\n            v-for=\"(item, index) in records\"\n            :key=\"index\"\n          >\n            <template v-slot:activator>\n              <div\n                class=\"d-flex align-center col-12 pa-0\"\n                style=\"margin-top: 14px\"\n              >\n                <div>\n                  <img\n                    v-if=\"address === item.signer\"\n                    :src=\"\n                      require(`@/assets/${$config.theme}/ico_sent_list.svg`)\n                    \"\n                  />\n                  <img\n                    v-else\n                    :src=\"\n                      require(`@/assets/${$config.theme}/ico_receive_list.svg`)\n                    \"\n                  />\n                </div>\n                <div\n                  class=\"flex-grow-1 ml-3 pb-1 d-flex align-center justify-space-between\"\n                >\n                  <div>\n                    <div class=\"font14\">\n                      {{\n                        address == item.signer\n                          ? item.recipient.substr(0, 6) +\n                            \"*****\" +\n                            item.recipient.substr(-6)\n                          : item.signer.substr(0, 6) +\n                            \"*****\" +\n                            item.signer.substr(-6)\n                      }}\n                    </div>\n                    <div class=\"color-grey SF-Text-Light\">\n                      <span class=\"font11\">\n                        {{ address == item.signer ? \"Sent\" : \"Received\" }} |\n                      </span>\n                      <time-ago\n                        class=\"time-ago\"\n                        :datetime=\"item.timestamp\"\n                        long\n                      ></time-ago>\n                    </div>\n                  </div>\n                  <div class=\"trans-outer\">\n                    <div\n                      class=\"font-weight-bold text-end color-grey font16 trans-rec DIN-Alternate-Bold\"\n                    >\n                      {{\n                        address == item.signer\n                          ? address == item.recipient\n                            ? \"self\"\n                            : \"-\" +\n                              $utils.toThousands(\n                                $utils.getBalance(\n                                  item.content.data.amount,\n                                  unity\n                                )\n                              )\n                          : \"+\" +\n                            $utils.toThousands(\n                              $utils.getBalance(item.content.data.amount, unity)\n                            )\n                      }}\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </template>\n            <div\n              class=\"d-flex justify-space-between ml-10 font12 addborder\"\n              style=\"padding:12px 0px 14px 0px\"\n            >\n              <div class=\"color-grey\">\n                <p>\n                  Transaction ID\n                </p>\n                <p class=\"rd-line2\">\n                  Status\n                </p>\n                <p>\n                  Transaction Fee\n                </p>\n              </div>\n              <div class=\"text-end ml-2\">\n                <p class=\"text-break\">\n                  <span :id=\"item.transactionId\">\n                    {{\n                      item.transactionId.substr(0, 6) +\n                        \"***\" +\n                        item.transactionId.substr(-6)\n                    }}\n                  </span>\n                  <copy-button\n                    :copy-id=\"item.transactionId\"\n                    :copy-content=\"item.transactionId\"\n                    :color=\"$config.theme\"\n                  ></copy-button>\n                </p>\n                <p class=\"rd-line2\">\n                  {{ item.status }}\n                </p>\n                <p>\n                  {{ item.transactionFee }}\n                </p>\n              </div>\n            </div>\n          </v-list-group>\n        </div>\n        <infinite-loading\n          :identifier=\"infiniteId\"\n          spinner=\"spiral\"\n          @infinite=\"infiniteHandler\"\n        >\n          <span slot=\"no-more\"></span>\n          <span\n            slot=\"no-results\"\n            style=\"position: relative; top: 10vh;font-weight: 300\"\n            >No Records</span\n          >\n        </infinite-loading>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TimeAgo from \"vue2-timeago\";\nimport { QrcodeStream } from \"vue-qrcode-reader\";\nimport { mapState, mapGetters } from \"vuex\";\nimport scanQrStyle from \"@/views/Home/scanQrStyle.vue\";\nimport crypto from \"@/libs1/crypto\";\nimport InfiniteLoading from \"vue-infinite-loading\";\nexport default {\n  data: () => ({\n    alias: \"\",\n    records: [],\n    lastKey: \"\",\n    lastType: \"all\",\n    isloading: false,\n    timer: \"\",\n    infiniteId: +new Date(),\n    address: \"\",\n    dialog: false,\n    scanOverlay: false,\n    scanOverlay1: false,\n    openCamera: \"off\",\n    qrImportInit: false,\n    editingAddress: false,\n    typetransation: \"All\",\n    txTypes: [\"All\", \"Income\", \"Out\"],\n    menuOn: false\n  }),\n\n  created() {\n    //get current address\n    this.address = this.wallets[this.currentIndex].address;\n    //get current alias\n    this.alias = JSON.parse(window.localStorage[\"alias\"])[this.currentIndex];\n    this.requestData();\n  },\n\n  mounted() {\n    this.timer = setTimeout(this.refreshTimer, 200000);\n    this.resizeRecords();\n  },\n\n  beforeDestroy() {\n    this.$utils.enableBodyScroll();\n    clearTimeout(this.timer);\n  },\n  watch: {\n    scanOverlay1(nv) {\n      this.$utils.lockScroll(nv);\n    }\n  },\n  computed: {\n    ...mapState([\"wallets\", \"currentIndex\", \"unity\"]),\n    ...mapGetters([\"balance\"])\n  },\n  methods: {\n    refreshTimer() {\n      if (this.isloading) return;\n      this.infiniteReload();\n      this.timer = setTimeout(this.refreshTimer, 200000);\n    },\n    typeChange(type) {\n      //change records type and then refresh records data\n      this.typetransation = type;\n      this.lastType = {\n        All: \"all\",\n        Income: \"in\",\n        Out: \"out\"\n      }[type];\n      this.refreshData();\n    },\n    resizeRecords() {\n      //adjust min-height of records-list\n      if (this.$refs[\"records-list\"].style.minHeight) return;\n      const bp = this.$vuetify.breakpoint;\n      //use window.innerHeight instead of vh\n      if (bp.sm) {\n        this.$refs[\"records-list\"].style.minHeight = `${window.innerHeight -\n          0.45 * window.innerWidth -\n          181}px`;\n      }\n      if (bp.md) {\n        this.$refs[\"records-list\"].style.minHeight = `${window.innerHeight -\n          385}px`;\n      }\n      if (bp.lg || bp.xl) {\n        this.$refs[\"records-list\"].style.minHeight = `${window.innerHeight -\n          396}px`;\n      }\n    },\n    infiniteHandler($state) {\n      //infinite-loading handler\n      this.isloading = true;\n      this.$axios\n        .get(\n          `token/transactions?tokenId=${this.$config.tokenId}&address=${this.address}&direction=${this.lastType}&startIndex=${this.lastKey}`,\n          { timeout: 60000 }\n        )\n        .then(res => {\n          if (res.data && res.data.result.Count) {\n            this.records = this.records.concat(res.data.result.Items);\n            if (res.data.result.LastEvaluatedKey) {\n              this.lastKey = res.data.result.LastEvaluatedKey;\n              $state.loaded();\n            } else {\n              this.lastKey = \"\";\n              $state.loaded();\n              $state.complete();\n            }\n          } else {\n            $state.complete();\n          }\n        })\n        .catch(err => {\n          if (err.response && err.response.status === 400) {\n            $state.complete();\n          }\n        })\n        .finally(() => {\n          this.isloading = false;\n        });\n    },\n    async requestData() {\n      try {\n        //request TOKEN info\n        this.$store.dispatch(\"update_token\", this.$config.tokenId);\n        this.requestBalance();\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    refreshData() {\n      //when refresh data, reload the infinite-loading component and reset the timer\n      if (this.isloading) return;\n      this.infiniteReload();\n      clearTimeout(this.timer);\n      this.timer = setTimeout(this.refreshTimer, 200000);\n    },\n    infiniteReload() {\n      //refresh token, balance and records\n      this.$store.dispatch(\"update_token\", this.$config.tokenId);\n      this.requestBalance();\n      this.records = [];\n      this.lastKey = \"\";\n      //change infiniteId to reload infinite-loading component\n      this.infiniteId += 1;\n    },\n    requestBalance() {\n      this.$store.dispatch(\"update_balance\", {\n        index: this.currentIndex,\n        tokenId: this.$config.tokenId\n      });\n    },\n    routerTransaction(type) {\n      this.$router.replace({\n        path: \"/home/\" + type\n      });\n    },\n    // store in window.localStorage\n    storeAlias() {\n      this.editingAddress = !this.editingAddress;\n      let alias = JSON.parse(window.localStorage[\"alias\"]);\n      let index = this.currentIndex;\n      if (this.alias.length <= 15) {\n        alias[index] = this.alias;\n        window.localStorage[\"alias\"] = JSON.stringify(alias);\n        this.$store.commit(\"update_wallet\", {\n          index: index,\n          alias: alias[index]\n        });\n      } else {\n        // alert(`Sorry! The name you wrote can't more than 15 character`);\n        this.alias = alias[index];\n      }\n    },\n    // scan qrcode\n    exitCamera() {\n      this.scanOverlay = !this.scanOverlay;\n      this.scanOverlay1 = !this.scanOverlay1;\n      this.openCamera = \"off\";\n    },\n    async onQrImportInit(promise) {\n      try {\n        this.qrImportInit = true;\n        await promise;\n        this.scanOverlay1 = true;\n      } catch (error) {\n        this.scanOverlay = false;\n        if (error.name === \"NotAllowedError\") {\n          alert(\"Please allow camera access permission\");\n        } else if (error.name === \"NotFoundError\") {\n          alert(\"No suitable camera device installed\");\n        } else if (error.name === \"NotSupportedError\") {\n          alert(\"Page is not served over HTTPS (or localhost)\");\n        } else if (error.name === \"NotReadableError\") {\n          alert(\"Maybe camera is already in use\");\n        } else if (error.name === \"OverconstarinedError\") {\n          alert(\"Pass constraints do not match any camera\");\n        } else {\n          alert(error);\n        }\n      } finally {\n        this.qrImportInit = false;\n      }\n    },\n    onQrDecode(decodeString) {\n      try {\n        console.log(decodeString);\n        if (!decodeString) return;\n        let receiveAddress = JSON.parse(decodeString);\n        console.log(receiveAddress);\n        if (\n          crypto.isValidAddress(receiveAddress.address, this.$config.NET_BYTE)\n        ) {\n          this.$store.commit(\"update_sendInfo\", {\n            recipient: receiveAddress.address,\n            amount: receiveAddress.amount,\n            description: receiveAddress.description\n          });\n          this.$router.replace({ path: \"/home/send\" });\n        }\n      } catch (err) {\n        console.error(err);\n        // alert(\"The qr code you scanned is illegal in the site\");\n        this.qrSignCamera = \"auto\";\n      }\n    }\n  },\n\n  components: {\n    InfiniteLoading,\n    TimeAgo,\n    QrcodeStream,\n    scanQrStyle\n  }\n};\n</script>\n<style scoped>\n.bal-inner {\n  overflow-x: scroll;\n  overflow-y: hidden;\n  line-height: 11.2vw;\n  height: 16vw;\n}\n.bal-outer {\n  height: 11.2vw;\n  overflow: hidden;\n}\n.trans-rec {\n  overflow-x: scroll;\n  overflow-y: hidden;\n  height: 35px;\n  line-height: 19px;\n  max-width: 29vw;\n}\n.trans-outer {\n  height: 19px;\n  overflow: hidden;\n}\n.font-size-09 {\n  font-size: 0.9rem;\n}\n.refresh:active {\n  opacity: 0.3;\n}\n.edi-address {\n  width: 30vw;\n  /* height: 15px; */\n  outline: none;\n  border-bottom: 1px solid white;\n}\n.scan-style {\n  z-index: 99;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n}\n.qrcode-waiting {\n  display: flex;\n  align-items: center;\n  margin: auto;\n  height: 100%;\n}\n.wallet-bg {\n  background: var(--urlHome);\n  background-size: cover;\n  border-radius: 12px;\n}\n.btn-tran div {\n  flex: 1;\n  text-align: center;\n  border: 1px solid #ebebeb;\n  border-radius: 8px;\n}\n.add-radius {\n  border-radius: 16px;\n}\n.addborder {\n  border-bottom: 1px solid #f2f2f2;\n  border-top: 1px solid #f2f2f2;\n}\n.v-dialog__content {\n  display: flex;\n  align-items: flex-end;\n}\n.v-dialog__content >>> .v-dialog--active {\n  margin: 0 3.2vw 12px 3.2vw;\n  color: red !important;\n}\n.v-list-group >>> .v-list-item:not(.v-list-item--disabled) {\n  padding-left: 0px !important;\n}\n.v-list-group >>> .v-list-group__header {\n  padding: 0px;\n}\n.records-off {\n  background-color: #f7f7f7;\n  color: grey;\n}\n.white-font {\n  color: #ffffff;\n}\n.record-menu-l {\n  font-size: 14px;\n  margin-left: 14px;\n  margin-right: 4px;\n}\n.record-menu-r {\n  font-size: 12px;\n  margin-right: 10px;\n}\n.title-balance {\n  font-size: 9.6vw;\n}\n.rd-line2 {\n  margin: 8px 0 8px 0;\n}\n\n.time-ago >>> .v-time-ago__text {\n  color: #9e9e9e;\n  font-size: 11px;\n  font-family: \"SFProText-Light\", \"SFProText\", \"SF Pro Text Light\";\n  font-weight: 300;\n}\n.send-card {\n  padding: 19px 0 18px 0;\n  margin-bottom: 0px;\n}\n\n.v-list-group >>> .v-list-group__header::before {\n  opacity: 0 !important;\n}\n</style>\n"],"sourceRoot":"src/views"}]}