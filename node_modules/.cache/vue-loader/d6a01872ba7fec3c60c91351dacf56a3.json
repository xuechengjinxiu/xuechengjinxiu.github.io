{"version":3,"sources":["Login.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+GA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Login.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div>\n    <LoginHeader\n      :title=\"$config.alias\"\n      guide=\"Create\"\n      ToPage=\"create\"\n    ></LoginHeader>\n    <div class=\"text-center maxWidth\">\n      <div class=\"font24 SF-Display-Medium mb-3 mt-3\">Sign in</div>\n      <div class=\"font15 SF-Text-Regular px20 mb28\">\n        Sign in or create a new account, or restore your account from backup\n      </div>\n      <v-text-field\n        v-model=\"password\"\n        type=\"password\"\n        solo\n        flat\n        label=\"Enter Password\"\n        :error=\"isPwdError\"\n        :error-messages=\"errorMsg\"\n        clearable\n      ></v-text-field>\n      <v-btn\n        block\n        color=\"primary\"\n        dark\n        class=\"text-none SF-Text-Medium\"\n        style=\"border-radius: 8px\"\n        height=\"48px\"\n        @click=\"Login\"\n      >\n        <div class=\"font18\">Sign in</div>\n      </v-btn>\n      <router-link to=\"restore\">\n        <v-btn\n          block\n          class=\"text-none primary--text white mt-4 SF-Text-Medium font18 btn-border\"\n          style=\"border-radius: 8px\"\n          height=\"48px\"\n        >\n          <div class=\"font18\">Restore</div>\n        </v-btn>\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport seedLib, { Seed } from \"@/libs1/seed\";\nexport default {\n  data() {\n    return {\n      password: \"\",\n      isPwdError: false,\n      errorMsg: \"\"\n    };\n  },\n  methods: {\n    Login() {\n      let cacheInfo = window.localStorage[\"encryptSeed\"];\n      let userInfo = cacheInfo;\n      try {\n        let seedWords = seedLib.decryptSeedPhrase(\n          JSON.parse(seedLib.decryptSeedPhrase(userInfo, this.password))\n            .encrSeed,\n          this.password\n        );\n        this.seed = new Seed(seedWords, 0, this.$config.NET_BYTE);\n        let walletCount = Number(window.localStorage[\"walletCount\"]);\n        this.$store.commit(\"ADD_INFO\", {\n          password: this.password,\n          seed: seedWords,\n          walletCount: walletCount\n        });\n        for (let i = 0; i < walletCount; i++) {\n          let seed = new Seed(seedWords, i, this.$config.NET_BYTE);\n          this.$store.commit(\"add_wallets\", {\n            index: i,\n            data: {\n              address: seed.address,\n              keyPair: seed.keyPair,\n              alias: JSON.parse(window.localStorage[\"alias\"])[i],\n              balance: \"\",\n              vsysBalance: 0,\n              transactionCount: \"\",\n              updateTime: \"\"\n            }\n          });\n        }\n        this.errorMsg = \"\";\n        this.isPwdError = false;\n      } catch (err) {\n        this.isPwdError = true;\n        this.errorMsg =\n          \"Password error, you can restore your account by your seed.\";\n      }\n      if (!this.isPwdError) {\n        if (this.$store.state.loginType === \"send\") {\n          this.$router.replace({\n            path: \"/home/shift\",\n            query: this.$route.query\n          });\n        } else {\n          this.$router.replace({ path: \"/home\" });\n        }\n      }\n    }\n  }\n};\n</script>\n<style scoped>\n.v-text-field >>> .v-input__slot {\n  border-radius: 8px;\n}\n.v-text-field >>> .v-label {\n  font-family: \"SFProText-Medium\", \"SFProText\", \"SF Pro Text Medium\";\n  font-weight: 500;\n  font-size: 15px;\n  color: #9e9e9e;\n  max-width: 100% !important;\n  margin-left: 4px;\n}\n.v-text-field >>> input {\n  margin-left: 4px;\n}\n.v-text-field >>> input {\n  padding: 0 !important;\n}\n</style>\n"]}]}