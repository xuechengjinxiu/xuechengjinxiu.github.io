n.bal-outer {\n  overflow: hidden;\n  height: 32px;\n}\n.wallet-card {\n  margin-bottom: 20px;\n  padding: 20px 20px 25px 20px;\n  position: relative;\n  // height: 44.8vw;\n}\n.card-icon {\n  position: absolute;\n  right: 5.33vw;\n  top: calc(50% - 37.5px);\n}\n.defaultWhite {\n  background: white;\n  background-size: cover;\n  border-radius: 16px;\n  .trans-records {\n    margin-top: 19px;\n    line-height: 20px;\n    color: #9e9e9e;\n  }\n}\n.selected-bg {\n  background: var(--urlShift);\n  background-size: cover;\n  border-radius: 16px;\n  color: white;\n  .trans-records {\n    color: white;\n  }\n}\n.set-round {\n  display: flex;\n  border-radius: 8px;\n  height: 44px;\n  div {\n    flex: 1;\n    border-radius: 8px;\n  }\n}\n",{"version":3,"sources":["Shift.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwLA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Shift.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <!--  <div>-->\n  <!--    <MainHeader title=\"Switch Account\"></MainHeader>-->\n  <!-- Transaction button -->\n  <div class=\"maxWidth\">\n    <div class=\"mx4 mt-6 SF-Text-Medium\">\n      <div class=\"grey lighten-3 set-round\">\n        <div\n          class=\"ma-1 py-2 d-flex align-center justify-center\"\n          @click=\"A_S(false)\"\n          :class=\"{ 'primary--text white': addressOrSeeds }\"\n        >\n          <img\n            v-if=\"addressOrSeeds\"\n            :src=\"require(`@/assets/${$config.theme}/ic_address_activited.svg`)\"\n          />\n          <img v-else src=\"@/assets/ic_address.svg\" />\n          &nbsp;&nbsp;\n          <span>Address</span>\n        </div>\n        <div\n          class=\"ma-1 py-2 d-flex align-center justify-center color-grey\"\n          :class=\"{ 'primary--text white': !addressOrSeeds }\"\n        >\n          <img\n            v-if=\"!addressOrSeeds\"\n            :src=\"require(`@/assets/${$config.theme}/ic_seeds_activited.svg`)\"\n          />\n          <img v-else src=\"@/assets/ic_seeds.svg\" />\n          &nbsp;&nbsp;\n          <span>Seeds</span>\n        </div>\n      </div>\n    </div>\n    <!-- Wallet components -->\n    <div style=\"overflow-x: hidden; margin-top:16px\" class=\"SF-Display-Regular\">\n      <div class=\"pa-0\">\n        <div\n          class=\"justify-space-between defaultWhite wallet-card\"\n          v-for=\"(item, index) in wallets\"\n          :key=\"index\"\n          @click=\"selectAddress(index)\"\n          :class=\"{ 'selected-bg': index === currentIndex }\"\n        >\n          <div class=\"pa-0\">\n            <div class=\"text-break font20\">\n              <span v-if=\"item.alias\" class=\"\">{{\n                `${item.alias}(${item.address.substr(\n                  0,\n                  4\n                )}***${item.address.substr(-4)})`\n              }}</span>\n              <span v-else class=\"\">{{\n                item.address.substr(0, 8) + \"******\" + item.address.substr(-8)\n              }}</span>\n            </div>\n            <div\n              class=\"SF-Display-Medium font15\"\n              style=\"margin-top: 16px;line-height: 18px\"\n            >\n              Available Balance\n            </div>\n            <div class=\"bal-outer\">\n              <div class=\"font28 mt-2 bal-inner DIN-Alternate-Bold\">\n                {{ $utils.toThousands($utils.getBalance(item.balance, unity)) }}\n              </div>\n            </div>\n            <div class=\"font15 trans-records\">\n              Transaction Records:\n              <span class=\"DIN-Alternate-Bold\" style=\"margin-left: 6px\">{{\n                item.transactionCount\n              }}</span>\n            </div>\n          </div>\n          <img\n            class=\"card-icon\"\n            v-if=\"index === currentIndex\"\n            src=\"@/assets/ic_card_check.svg\"\n          />\n        </div>\n        <div\n          v-if=\"Object.keys(wallets).length < 10\"\n          class=\"col-12 py-6 d-flex justify-center defaultWhite\"\n          @click=\"addAddress()\"\n        >\n          <img width=\"50\" height=\"50\" src=\"@/assets/ic_add.svg\" />\n        </div>\n      </div>\n    </div>\n  </div>\n  <!--  </div>-->\n</template>\n\n<script>\nimport seedLib, { Seed } from \"@/libs1/seed\";\nimport { mapState } from \"vuex\";\nexport default {\n  name: \"shift\",\n  data() {\n    return {\n      seedWords: \"\",\n      addressOrSeeds: true\n    };\n  },\n  created() {\n    // init seedWords\n    let userInfo = window.localStorage[\"encryptSeed\"];\n    try {\n      this.seedWords = seedLib.decryptSeedPhrase(\n        JSON.parse(seedLib.decryptSeedPhrase(userInfo, this.password)).encrSeed,\n        this.password\n      );\n    } catch (err) {\n      console.log(err);\n    }\n    // update wallets\n    this.$store.dispatch(\"update_token\", this.$config.tokenId);\n    for (let i in this.wallets) {\n      this.$store.dispatch(\"update_balance\", {\n        index: i,\n        tokenId: this.$config.tokenId\n      });\n    }\n  },\n  computed: {\n    ...mapState([\"wallets\", \"currentIndex\", \"unity\", \"password\"])\n  },\n  methods: {\n    addAddress() {\n      if (Object.keys(this.wallets).length > 9) return;\n      // alert(`Sorry! You can't create more than 10 wallets`);\n      let index = Object.keys(this.wallets).length;\n      let seed = new Seed(this.seedWords, index, this.$config.NET_BYTE);\n      this.addWalletInfo(seed);\n      // requeset new wallet transaction data\n      this.$store.dispatch(\"update_balance\", {\n        index: index,\n        tokenId: this.$config.tokenId\n      });\n    },\n\n    addWalletInfo(seed) {\n      //Add Store Info\n      this.$store.commit(\"add_wallets\", {\n        index: window.localStorage[\"walletCount\"],\n        data: {\n          address: seed.address,\n          keyPair: seed.keyPair,\n          alias: \"\",\n          balance: 0,\n          vsysBalance: 0,\n          transactionCount: 0,\n          updateTime: new Date().getTime()\n        }\n      });\n      this.$store.commit(\"ADD_INFO\", {\n        walletCount: Number(window.localStorage[\"walletCount\"])\n      });\n      //Add LocalStorage Info\n      let aliasObj = JSON.parse(window.localStorage[\"alias\"]);\n      aliasObj[window.localStorage[\"walletCount\"]] = \"\";\n      window.localStorage[\"alias\"] = JSON.stringify(aliasObj);\n      window.localStorage[\"walletCount\"]++;\n    },\n\n    selectAddress(key) {\n      // Storage vuex\n      this.$store.commit(\"ADD_INFO\", {\n        currentIndex: Number(key)\n      });\n      if (this.$store.state.loginType === \"send\") {\n        this.$router.replace({ path: \"/home/send\", query: this.$route.query });\n      } else {\n        this.$router.replace({ path: \"/home\" });\n      }\n    },\n\n    A_S(as) {\n      if (this.addressOrSeeds == as) this.addressOrSeeds = !this.addressOrSeeds;\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n.bal-inner {\n  line-height: 28px;\n  height: 35px;\n  overflow-x: scroll;\n  overflow-y: hidden;\n}\n.bal-outer {\n  overflow: hidden;\n  height: 32px;\n}\n.wallet-card {\n  margin-bottom: 20px;\n  padding: 20px 20px 25px 20px;\n  position: relative;\n  // height: 44.8vw;\n}\n.card-icon {\n  position: absolute;\n  right: 5.33vw;\n  top: calc(50% - 37.5px);\n}\n.defaultWhite {\n  background: white;\n  background-size: cover;\n  border-radius: 16px;\n  .trans-records {\n    margin-top: 19px;\n    line-height: 20px;\n    color: #9e9e9e;\n  }\n}\n.selected-bg {\n  background: var(--urlShift);\n  background-size: cover;\n  border-radius: 16px;\n  color: white;\n  .trans-records {\n    color: white;\n  }\n}\n.set-round {\n  display: flex;\n  border-radius: 8px;\n  height: 44px;\n  div {\n    flex: 1;\n    border-radius: 8px;\n  }\n}\n</style>\n"]}]}