        {{ item.value }}\n        </div>\n      </v-toolbar-title>\n    </div>\n    <div class=\"add-border d-none d-md-flex\"></div>\n    <div class=\"d-none d-md-flex\" style=\"height: 10px\"></div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    source: String,\n    title: String\n  },\n  data: () => ({\n    drawer: false,\n    drawerHeight: window.innerHeight\n  }),\n  created() {\n    this.menuLists = this.menuListsComputed();\n  },\n  mounted() {\n    window.addEventListener(\"resize\", () => {\n      this.drawerHeight = window.innerHeight;\n    });\n  },\n  watch: {\n    drawer(nv) {\n      //update drawer state in store\n      this.$store.commit(\"update_header\", nv);\n    }\n  },\n  methods: {\n    menuListsComputed() {\n      let configTheme = this.$config.theme;\n      return [\n        {\n          value: \"Wallet Home\",\n          path: \"/home/home\",\n          name: \"/home\",\n          // use require() or put imgs into './public'\n          // img_1: 'img/cyan/ic_assets_activited.svg',\n          // img_2: 'img/ic_assets_normal.svg'\n          img_1: require(`@/assets/${configTheme}/ic_assets_activited.svg`),\n          img_2: require(\"@/assets/ic_assets_normal.svg\"),\n          img_3: require(\"@/assets/ic_assets_grey.svg\")\n        },\n        {\n          value: \"My Information\",\n          path: \"/home/info\",\n          img_1: require(`@/assets/${configTheme}/ic_info_activited.svg`),\n          img_2: require(\"@/assets/ic_info_normal.svg\"),\n          img_3: require(\"@/assets/ic_info_grey.svg\")\n        },\n        {\n          value: \"Switch Account\",\n          path: \"/home/shift\",\n          img_1: require(`@/assets/${configTheme}/ic_wallet_activited.svg`),\n          img_2: require(\"@/assets/ic_wallet_normal.svg\"),\n          img_3: require(\"@/assets/ic_wallet_grey.svg\")\n        },\n        {\n          value: \"Show VSYS\",\n          path: \"\",\n          img_1: require(\"@/assets/ic_vsys_normal.svg\"),\n          img_2: require(\"@/assets/ic_vsys_normal.svg\"),\n          img_3: require(\"@/assets/ic_vsys_grey.svg\")\n        },\n        {\n          value: \"Setting\",\n          path: \"/home/setting\",\n          img_1: require(`@/assets/${configTheme}/ic_setting_activited.svg`),\n          img_2: require(\"@/assets/ic_setting_normal.svg\"),\n          img_3: require(\"@/assets/ic_setting_grey.svg\")\n        },\n        {\n          value: \"Logout\",\n          path: \"/\",\n          img_1: \"\",\n          img_2: require(\"@/assets/ic_logout_normal.svg\"),\n          img_3: require(\"@/assets/ic_logout_normal.svg\")\n        }\n      ];\n    },\n    routerPage(page, mobile) {\n      if (!page) return;\n      if (this.$route.path == page && mobile) {\n        this.drawer = !this.drawer;\n      } else if (this.$route.path == page && !mobile) {\n        return;\n      } else {\n        this.$router.replace({ path: page });\n      }\n    }\n  }\n};\n</script>\n<style lang=\"scss\">\n.add-border {\n  width: 100%;\n  height: 20px;\n  left: 0px;\n  position: absolute;\n  box-shadow: 0 2px 0 #ebebeb;\n  pointer-events: none;\n}\n.v-list-item__action {\n  margin-right: 16px !important;\n}\n.logout {\n  max-height: 24px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}